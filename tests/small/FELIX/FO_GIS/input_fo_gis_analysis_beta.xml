    <Parameter name="Basal Node Layered Fields Ranks"       type="Array(int)"    value="{1}"/>
    <ParameterList name="Required Fields Info">
     <Parameter name="Number Of Fields" type="int" value="4"/>
      <ParameterList name="Field 0">
